{
  "$schema": "https://github.com/grayjay-sources/grayjay-sources.github.io/blob/main/sources.schema.json#/definitions/Source",
  "name": "Pietsmiet.de",
  "description": "GrayJay source for Pietsmiet.de",
  "author": "Bluscream",
  "authorUrl": "https://bluscream.github.io",
  "platformUrl": "https://www.pietsmiet.de/",
  "tags": [],
  "nsfw": false,
  "sourceUrl": "https://github.com/grayjay-sources/grayjay-source-pietsmietde/releases/latest/download/config.json",
  "repositoryUrl": "https://github.com/grayjay-sources/grayjay-source-pietsmietde",
  "scriptUrl": "./script.js",
  "changelogUrl": "https://raw.githubusercontent.com/grayjay-sources/grayjay-source-pietsmietde/master/changelog.md",
  "version": 46,
  "iconUrl": "https://yt3.ggpht.com/-nm49M5AVi64/AAAAAAAAAAI/AAAAAAAAAAA/h0Scy91ZiZQ/s900-c-k-no-rj-c0xffffff/photo.jpg",
  "id": "82b8fefc-24ae-11ee-be56-0242aa120002",
  "packages": [
    "Http",
    "Utilities"
  ],
  "allowEval": false,
  "allowUrls": [
    "pietsmiet.de",
    "www.pietsmiet.de",
    "minopia.de",
    "*.minopia.de",
    "ytapi.minopia.de",
    "ytapi2.minopia.de"
  ],
  "subscriptionRateLimit": 120,
  "authentication": {
    "loginUrl": "https://www.pietsmiet.de/auth/login",
    "cookiesToFind": [
      "XSRF-TOKEN",
      "psde_auth3"
    ],
    "userAgent": "Mozilla/5.0 (Linux; Android 12) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.6099.230 Mobile Safari/537.36",
    "loginWarning": "Login does not work (yet)."
  },
  "settings": [
    {
      "variable": "use_yt_proxy",
      "name": "Enable Youtube Proxy",
      "description": "Will make the source request the youtube proxy server to get additional metadata from youtube",
      "type": "Boolean",
      "default": "true",
      "warningDialog": "This increases loading times significantly.\n\nTry disabling if you have issues when loading videos."
    },
    {
      "variable": "yt_proxy_server",
      "name": "Youtube Proxy Server",
      "description": "Which Youtube Proxy Server to use",
      "type": "Dropdown",
      "default": "0",
      "options": ["ytapi.minopia.de","ytapi2.minopia.de"]
    },
    {
      "variable": "merge_yt_metrics",
      "name": "Merge PSDE and YT metrics (requires YTProxy)",
      "description": "Will merge the metrics from pietsmiet.de and youtube.com (views, likes, dislikes, etc) when available through PSProxy",
      "type": "Boolean",
      "default": "true"
    }
  ]
}
